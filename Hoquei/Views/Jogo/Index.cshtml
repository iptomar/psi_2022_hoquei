@model IEnumerable<Hoquei.Models.Jogador>

@{ ViewData["Title"] = "Index"; }
<head>
    <link rel="stylesheet" href="~/css/ImagensFilmes.css" />
</head>
<h1>Lista de Jogadores</h1>

@foreach (var item in Model) {

<div class="Carro">
    <a asp-action="Details" asp-route-id="@item.Num_Fed">
        <div class="overlay">
            <div class="text">
                @Html.DisplayFor(modelItem => item.Name)
            </div>
        </div>
    </a>
</div>}


